<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".myratingstab">


    <TextView
        android:id="@+id/totalfavoritescount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="180dp"
        android:text="totalfavoritescount"
        android:textColor="#FF0000"
        android:textSize="24sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/articlepublishedcount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="89dp"
        android:text="articlepublishedcount"
        android:textColor="#FF0000"
        android:textSize="24sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/totalfavorites"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="140dp"
        android:text="totalfavorites"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/articlepublished"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="40dp"
        android:text="articlepublished"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/nameauth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:text="nameauth"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"/>
</FrameLayout>